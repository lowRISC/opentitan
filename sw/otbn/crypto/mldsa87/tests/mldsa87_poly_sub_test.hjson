// Check that x - x mod q = 0 (base) and
// check that 0 - x mod q = q - x (underflow)
// for x in [1, 256] and q = 8380417.

{
  "entrypoint": "main",
  "input": {
    "dmem": {
       // [1, 2, ..., 256]
       "_poly_sub_a_base": "",
       // [1, 2, ..., 256]
       "_poly_sub_b_base": "",

       // [0, 0, ..., 0]
       "_poly_sub_a_underflow": "",
       // [1, 2, ..., 256]
       "_poly_sub_b_underflow": "",
    }
  },
  "output": {
    "dmem": {
       // [0, 0, ..., 0]
       "_poly_sub_c_base": "",

       // [q-1, q-2, ..., q-256]
       "_poly_sub_c_underflow": ""
    }
  }
}
