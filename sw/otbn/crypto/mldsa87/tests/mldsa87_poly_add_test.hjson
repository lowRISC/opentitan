// Check that x + x mod q = 2 * x (base case) and
// check that q - 1 + x mod q = x (overflow)
// for x in [1, 256] and q = 8380417.

{
  "entrypoint": "main",
  "input": {
    "dmem": {
       // [1, 2, ..., 256]
       "_poly_add_a_base": "",
       // [1, 2, ..., 256]
       "_poly_add_b_base": "",

       // [1, 2, ..., 256]
       "_poly_add_a_overflow": "",
       // [q-1, q-1, ..., q-1]
       "_poly_add_b_overflow": ""
    }
  },
  "output": {
    "dmem": {
       // [2, 4, ..., 512]
       "_poly_add_c_base": "",

       // [0, 1, ..., 255]
       "_poly_add_c_overflow": ""
    }
  }
}
